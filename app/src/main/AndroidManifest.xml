<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.capiot.expensemanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddExpense" />
        <activity android:name=".AddExpenseInCategory" />
        <activity android:name=".ViewExpense" />
        <activity android:name=".Graph" />
        <activity android:name=".userOperations" />
        <activity android:name=".userExpenditure" />
        <activity android:name=".SetExpenseLimits" />
        <activity android:name=".showExpenseLimit"></activity>
    </application>

</manifest>